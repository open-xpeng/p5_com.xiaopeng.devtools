        <TextView android:textSize="@dimen/gps_text" android:textColor="@android:color/white" android:id="@+id/gps_location_info_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="64dp" android:layout_marginTop="32dp" android:layout_marginRight="64dp" android:layout_below="@+id/gps_location_header" android:layout_centerVertical="true"/>
    </RelativeLayout>
    <include android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="16dp" android:layout_above="@+id/layout_common" layout="@layout/layout_gps_reboot"/>
    <LinearLayout android:id="@+id/layout_common" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="16dp" android:layout_alignParentBottom="true">
        <include android:id="@+id/layout_factory_test_common" layout="@layout/layout_factory_test_common"/>
        <include layout="@layout/layout_aftersales_common"/>
    </LinearLayout>
</RelativeLayout>
